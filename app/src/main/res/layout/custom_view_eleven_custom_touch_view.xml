<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <com.example.seniorcustomview.customview11.view.customtouch.CustomTouchLayout android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#2b2b2b">

        <com.example.seniorcustomview.customview11.view.customtouch.CustomTouchView
            android:id="@+id/custom_touch_view"
            android:layout_width="300dp"
            android:layout_height="300dp"
            android:layout_centerInParent="true"
            android:background="#ff00ff"
            />


    </com.example.seniorcustomview.customview11.view.customtouch.CustomTouchLayout>


</androidx.constraintlayout.widget.ConstraintLayout>